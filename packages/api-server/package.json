{
  "name": "@godwoken-web3/api-server",
  "version": "0.2.0-rc1",
  "private": true,
  "scripts": {
    "start": "concurrently \"tsc -w\" \"DEBUG=godwoken-web3-api:server nodemon ./bin/www\"",
    "test": "ava",
    "fmt": "prettier --write \"{migrations,tests}/**/*.js\" app.js src/* package.json",
