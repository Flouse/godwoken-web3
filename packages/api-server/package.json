    "ckb-js-toolkit": "^0.10.2",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ethereumjs-util": "^7.0.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "immutable": "^4.0.0-rc.12",
    "jayson": "~3.4.4",
    "keccak256": "^1.0.2",
    "knex": "^0.95.7",
    "leveldown": "^6.0.0",
    "levelup": "^5.0.1",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "rlp": "^2.2.6",
    "secp256k1": "^4.0.2"
  },
  "devDependencies": {
    "@types/leveldown": "^4.0.2",
    "@types/levelup": "^4.3.1",
    "@types/secp256k1": "^4.0.2",
    "ava": "^3.15.0",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  }
